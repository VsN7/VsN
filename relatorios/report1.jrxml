	<queryString>
		<![CDATA[
(SELECT
     (despesa.descricao, ordemServico.servico) as "s" , despesa.valor, ordemServico.cliente, ordemServico.valor
FROM
     `despesa` despesa,
     `ordemServico` ordemServico
WHERE
     despesa.situacao = "ABERTO")
UNION
(
SELECT
     despesa.descricao, despesa.valor, ordemServico.servico, ordemServico.cliente, ordemServico.valor
FROM
     `despesa`,
     `ordemServico`
WHERE
     ordemServico.situacao = "FECHADO"
)]]>
